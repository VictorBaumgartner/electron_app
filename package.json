{
  "name": "helloworld",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
  },
  "build": {
    "appId": "com.example.helloworld",
    "productName": "HelloWorld",
    "mac": {
      "target": "dmg"
    },
    "extraResources": [
      {
        "from": "./python-portable/",
        "to": "python-portable",
        "filter": ["**/*"]
      },
      {
        "from": "./script.py",
        "to": "script.py"
      }
    ]
  }
}